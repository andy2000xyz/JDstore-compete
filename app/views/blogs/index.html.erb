

  <h2>作品集</h2><br>

  <div class="container" style="height: 600px">
    <div class="row">
      <div class="col-md-12">
        <ul>
          <% @blogs.each do |blog| %>
            <li>
              <%= link_to blog.title, blog_path(blog) %>
              <%= blog.created_at.strftime("%Y/%m/%d %I:%M %p") %>
            </li>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
